      </form>
   </div>
   </div>

   <div class="wrap_box-buttom">
      <div class="wrap_box-buttom-a">
         <table>
            <thead>
               <tr>
                  <th>품목명</th>
                  <th>운송장번호</th>
                  <th>조회</th>
                  <th>삭제</th>
               </tr>
            </thead>

            <tbody>
               <% if (users.length> 0) { %>
                  <% users.forEach(function(user) { %>
                     <% if (user.company !==null && user.number !==null) { %>
                        <tr>

                           <td>
                              <%= user.itemname %>
                           </td>

                           <td>
                              <%= user.number %>
                           </td>

      </div>
      <form action="http://info.sweettracker.co.kr/tracking/4" method="post">

         <div class="form-group">
            <div class="behind" style="display: none;">
               <input type="text" class="form-control" id="t_key" name="t_key" value="API키 넣기">
            </div>
         </div>
         <div class="form-group">
            <div class="behind" style="display: none;">
               <input type="text" class="form-control" name="t_code" id="t_code" value="<%= user.company%>">
            </div>
         </div>
         <div class="form-group">
            <div class="behind" style="display: none;">
               <input type="text" class="form-control" name="t_invoice" id="t_invoice" value="<%= user.number%>">
            </div>
         </div>

         <td class="detailSearch">
            <form action="/detailsearch" method="POST">
               <input type="hidden" name="itemname" value="<%= user.itemname %>">
               <input type="hidden" name="number" value="<%= user.number %>">
               <input type="hidden" name="company" value="<%= user.company %>">
               <button type="submit" class="btn2">상세조회</button>
            </form>
         </td>
      </form>
      
      <td class="delete">
         <form action="/delete" method="POST">
            <input type="hidden" name="itemname" value="<%= user.itemname %>">
            <input type="hidden" name="number" value="<%= user.number %>">
            <input type="hidden" name="company" value="<%= user.company %>">
            <input type="hidden" name="id" value="<%= user.id %>">
            <input type="hidden" name="phone" value="<%= user.phone %>">
            <button type="submit" class="btn2"><img
                  src="https://github.com/DeliveryGo/UI-design/blob/main/garbage.png?raw=true" alt="삭제"></button>
